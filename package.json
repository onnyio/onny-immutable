{
  "name": "onny-immutable",
  "version": "0.0.14",
  "description": "Onny utils for immutability",
  "main": "./dist/index.js",
  "scripts": {
    "build": "rm -rf .\\dist&&babel src --presets babel-preset-es2015 --out-dir dist",
    "prepublish": "npm run build",
    "lint": "eslint ./src --ext .js --format ./node_modules/eslint-config-onny/eslintFormat",
    "lint-fix": "eslint ./src --ext .js --format ./node_modules/eslint-config-onny/eslintFormat --fix",
    "lint-fix_test": "eslint ./test --ext .js --format ./node_modules/eslint-config-onny/eslintFormat --fix",
    "test_local": "mocha --require babel-polyfill --compilers js:babel-register",
    "test": "nyc --require babel-register --require babel-polyfill mocha && nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onnyio/onny-immutable.git"
  },
  "keywords": [
    "onny",
    "immutable"
  ],
  "author": "Isaac Marotte <isaac@onny.io>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/onnyio/onny-immutable/issues"
  },
  "homepage": "https://github.com/onnyio/onny-immutable#readme",
  "dependencies": {
    "immutability-helper": "^2.6.6",
    "onny-utils": "^1.0.9",
    "onny-validate": "0.0.6"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^8.2.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "eslint": "^4.18.2",
    "eslint-config-onny": "^1.0.2",
    "mocha": "^5.0.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.5.0"
  }
}
